<Window x:Name="Type1" x:Class="kyrovaya.xamls.Type"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kyrovaya.xamls"
        mc:Ignorable="d"
        Title="ТипыКомплектующего" Loaded="Type1_Loaded" Height="450" Width="800" Icon="/132706_pinion_control_application_preferences_generator_icon.png">
    <Grid>
        <DataGrid x:Name="dg1" IsReadOnly="True" AutoGenerateColumns="False" Margin="10,0,10,78">
            <DataGrid.Columns>
                <DataGridTextColumn Width="10*" Header="IDтипаКомплектующего" Binding="{Binding IdтипаКомплектующего}"/>
                <DataGridTextColumn Width="14*" Header="НазваниеТипаКомплектующего" Binding="{Binding НазваниеТипаКомплектующего}"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button x:Name="btnCancel_Click" Click="btnCancel1_Click" Content="Закрыть" HorizontalAlignment="Center" Margin="0,386,0,0" VerticalAlignment="Top" Height="27" Width="82"/>
    </Grid>
</Window>

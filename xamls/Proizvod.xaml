<Window x:Name="Proizvod1" x:Class="kyrovaya.Proizvod"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kyrovaya"
        mc:Ignorable="d"
        Title="Производители" Loaded="Proizvod1_Loaded" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dg1" IsReadOnly="True" AutoGenerateColumns="False" Margin="10,0,10,78">
            <DataGrid.Columns>
                <DataGridTextColumn Width="10*" Header="IDпроизводителя" Binding="{Binding Idпроизводителя}"/>
                <DataGridTextColumn Width="14*" Header="НазваниеПроизводителя" Binding="{Binding НазваниеПроизводителя}"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button x:Name="btnCancel_Click" Click="btnCancel1_Click" Content="Закрыть" HorizontalAlignment="Center" Margin="0,386,0,0" VerticalAlignment="Top" Height="27" Width="82"/>
    </Grid>
</Window>
